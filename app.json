{
  "name": "Video encoding bot",
  "description": "A Telegram based encoding bot for you!",
  "keywords": [
    "telegram"
  ],
  "repository": "https://github.com/NotDream72/Video-Encoder-Bot",
  " herokustack": "set stack 20",
  "env":{
        "API_ID":{
            "description":"Get this value from https://my.telegram.org"
        },
        "API_HASH":{
            "description":"Get this value from https://my.telegram.org"
        },
        "SUDO_USERS":{
            "description":"sudo users for your bot separate them by spaces uwu"
        },
        "BOT_TOKEN":{
            "description":"Get this from https://t.me/botfather and enable Inline Mode"
        },
        "RESOLUTION":{
            "description":"do nothing"
        },
        "PRESENT":{
            "description":"set it as sf"
        },
        "TUNE":{
            "description":"set it to film"
        },
        "AUDIO":{
            "description":"set as opus"
        },
        "CRF":{
            "description":"recommended value is 29"
        },
        "UPLOAD_AS_DOC":{
            "description":"put 1 or 0"
       },
       "DOWNLOAD_DIR":{
            "description": "put this as VideoEncoder/utils/downloads/"
       },
       "ENCODE_DIR":{
            "description": "Put this as VideoEncoder/utils/encodes/"
       },
      "buildpacks"buildpacks": [
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
    },
    {
      "url": "heroku/python"
      }
    }
}
